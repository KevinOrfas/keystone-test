<div class="container">
  <div class="panel panel-primary">
    <div class="panel-heading">Tickets</div>
    <div class="panel-body">
      <p>These are list of tickets in the system.</p>
    
    <table class="table table-striped">
      {{#each data.tickets}}
      <tr>
        <td>
          <div class="col-md-1">
            <span class="label label-info pull-right">{{status}}</span>
          </div>
          <a href="{{url}}"><b>{{capitalize title}}</b> </a>
          <ul class="ticket-meta">
            <li><small>Status</small> <a href="" rel="tag">{{status}}</a></li>
            <li><small>Priority</small> <a href="" rel="tag">{{priority}}</a></li>
            <li><small>Category</small> <a href="" rel="tag">{{category}}</a></li>
            <li><small>Last Updated</small> <abbr title="" class="last-updated">{{date createdAt}}</abbr></li>
          </ul>
        </td>
      </tr>
      {{/each}}
    </table>
    </div>
    <div class="panel-footer"></div>
  </div>
</div>